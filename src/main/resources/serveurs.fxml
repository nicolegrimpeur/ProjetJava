<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="screensControllers.Serveurs">
    <children>
        <Label layoutX="322.0" layoutY="14.0" text="Prise de commande">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Button disable="true" layoutX="258.0" layoutY="63.0" mnemonicParsing="false" text="Commande classique"
                onAction="#clickMenuClassique" fx:id="btnMenuClassique"/>
        <Button layoutX="418.0" layoutY="63.0" mnemonicParsing="false" text="Commande 100 ans"
                onAction="#clickMenuCentAns" fx:id="btnMenuCentAns"/>
        <Button layoutX="377.0" layoutY="561.0" mnemonicParsing="false" onAction="#submit" text="Submit"/>
        <TreeTableView layoutX="500.0" layoutY="100.0" prefHeight="450.0" prefWidth="300.0"
                       fx:id="treeTableAffichageMenu" onMouseClicked="#clickPanier">
        </TreeTableView>
        <ListView fx:id="listPlats" layoutY="100.0" prefHeight="450.0" prefWidth="300.0"
                  onMouseClicked="#clickPlatBoisson"/>
        <ListView fx:id="listBoissons" layoutX="300.0" layoutY="100.0" prefHeight="450.0" prefWidth="200.0"
                  onMouseClicked="#clickPlatBoisson"/>
        <Label layoutX="14.0" layoutY="19.0" text="Serveur :">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <ChoiceBox fx:id="serveurChoiceBox" layoutX="75.0" layoutY="17.0" onAction="#changeServeurChoiceBox"
                   prefWidth="150.0"/>
    </children>
</AnchorPane>
